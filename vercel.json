{
    "version": 2,
    "functions": {
        "api/app.py": {
            "includeFiles": "uploads/**"
        }
    },
    "routes": [
        {
            "src": "/file/(.*)",
            "dest": "api/app.py"
        },
        {
            "src": "/upload",
            "dest": "api/app.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ]
}
